<%= @install_dir %>/var/dionaea/dionaea.log {
    daily
    missingok
    rotate 14
    compress
    postrotate
        /etc/init.d/dio restart > /dev/null
    endscript
}

<%= @install_dir %>/var/dionaea/logsql.sqlite {
    monthly
    missingok
    rotate 12
    compress
    postrotate
        /etc/init.d/dio restart > /dev/null
    endscript
}
