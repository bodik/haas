{
	"url": "<%= @warden_server_url_real %>",
	"name": "<%= @w3c_name %>",
	"idstore": "warden_client.id",
	"timeout": 60,
	"get_events_limit": 1000,

        "certfile": "<%= @install_dir %>/racert/<%= @w3c_name %>.crt",
        "keyfile": "<%= @install_dir %>/racert/<%= @w3c_name %>.key",
        "cafile": "<%= @install_dir %>/racert/cachain.pem",

	"errlog": {"level": "debug"},
	"syslog": {"socket": "/dev/log", "facility": "local7", "level": "info"}
}
