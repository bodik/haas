<%= @install_dir %>/warden/*log {
	rotate 60
	daily
	missingok
	compress
	sharedscripts
	prerotate
		/usr/bin/find <%= @install_dir %>/log/ <%= @install_dir %>/dl/ -type f -ctime +<%= @log_history %> -delete 1>/dev/null 2>/dev/null
		/bin/sh <%= @install_dir %>/bin/mysql-clean.sh 1>/dev/null 2>/dev/null
	endscript
}

