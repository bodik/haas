[honeypot]
hostname = <%= @hostname %>

[ssh]
version = <%= @version_string_real %>
listen_endpoints = tcp:<%= @cowrie_port %>:interface=0.0.0.0

[output_mysql]
enabled = true
host = <%= @mysql_host %>
database = <%= @mysql_db %>
username = <%= @mysql_db %>
password = <%= @mysql_password_real %>
port = <%= @mysql_port %>
debug = false
